          <li><%= user.full_name %>  <a href="<%= edit_user_path(user) %>" class="uk-icon-link" uk-icon="file-edit"></a></li>
        <% end %>
			</ul>
		</li>
		<li>Content 3</li>
	</ul>
</div>
